<?xml version="1.0" encoding="UTF-8"?>
<ehcache>

	<diskStore path="java.io.tmpdir/hibernate/wbs" />

	<!-- DefaultCache setting. -->
	<defaultCache maxElementsInMemory="10000" memoryStoreEvictionPolicy="LRU" eternal="false"
		timeToIdleSeconds="300" timeToLiveSeconds="300" overflowToDisk="false" diskPersistent="false" />

	<!-- Special objects setting. -->
	<cache name="com.wbs.entity.system.User" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" eternal="true" diskPersistent="false" overflowToDisk="true" maxElementsOnDisk="1000000" />
	<cache name="com.wbs.entity.system.User.roleList" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" eternal="true" diskPersistent="false" overflowToDisk="true" maxElementsOnDisk="1000000" />
	<cache name="com.wbs.entity.system.Role" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" eternal="true" diskPersistent="false" overflowToDisk="true" maxElementsOnDisk="1000000" />
	<cache name="com.wbs.entity.system.Role.authorityList" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" eternal="true" diskPersistent="false" overflowToDisk="true" maxElementsOnDisk="1000000" />
	<cache name="com.wbs.entity.system.Authority" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" eternal="true" diskPersistent="false" overflowToDisk="true" maxElementsOnDisk="1000000" />
	<cache name="com.wbs.entity.system.Resource" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" eternal="true" diskPersistent="false" overflowToDisk="true" maxElementsOnDisk="1000000" />
	<cache name="com.wbs.entity.system.Resource.authorityList" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" eternal="true" diskPersistent="false" overflowToDisk="true" maxElementsOnDisk="1000000" />
</ehcache>
